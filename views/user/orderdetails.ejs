                            <% if(order.orderStatus === 'Cancelled') { %>
                                <h3 class="cart-page-title mb-0 text-danger">Your Cancelled Order Details</h3>
                            <% } else if(order.orderStatus === 'Delivered') { %>
                                <h3 class="cart-page-title mb-0 text-success">Your Delivered Order Details</h3>
                            <% } else if(order.orderStatus === 'Returned') { %>
                                <h3 class="cart-page-title mb-0 text-danger">Your Returned Order Details</h3>
                            <% } else { %>
                                <div>
                                    <h3 class="cart-page-title mb-3">Your Order Details</h3>
                                    <h3 class="ms-2 h5 mb-0 h6">Status: <%= order.orderStatus %></h3>
                                </div>
                            <% } %>
                            <div class="d-flex">
                                <% if(order.orderStatus !== 'Cancelled' && order.orderStatus !== 'Delivered' && order.orderStatus !== 'Returned') { %>
                                    <button class="btn btn-primary me-2"
                                        onclick="cancelBulkOrder(event, '<%= order.id %>')"
                                        style="background-color: #007bff; color: #fff; width: auto; height: auto;">
                                        <p class="p-3 mb-0">
                                            Cancel Order
                                        </p>
                                    </button>
                                <% } %>
                                <button class="btn btn-primary"
